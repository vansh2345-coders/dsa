#include <iostream>
using namespace std;


int main() {
    int temp;
    int arr[5]={1,2,3,4,5};
    int n=5;
    int i,j;
    for( i=0, j=n-1;i<n/2,j>=n/2;i++,j--){
        temp=arr[i];
        arr[i]=arr[j];
        arr[j]=temp;
    }
    for(int i=0;i<n;i++){
        cout<<arr[i];
    }
    
    return 0;
}
